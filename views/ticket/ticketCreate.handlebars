<form action="/ticketCreate" method="post">
  Titulo:
  <input type="text" name="titulo" />
  <br /><br />
  Descricao:
  <input type="text" name="descricao" />
  <br /><br />
  Categoria:
  <select name="categoriaId">
    {{#each categoria}}
      <option value={{dataValues.id}}>{{dataValues.nome}}</option>
    {{/each}}
  </select>
  <br /><br />
  <input type="submit" value="Relatar problema" />
</form>